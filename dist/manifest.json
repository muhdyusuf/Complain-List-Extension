{
    "name":"Borang Job Extensions",
    "description":"dynamiclly create jobsheet uisng data from site",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker":"background.js"
    },
    "content_scripts":[
        {
            "matches":["http://www.syarikatjnl.com/jknshelpdesk/complaint_list.php","http://www.syarikatjnl.com/jknshelpdesk/complaint_list.php?archived=1"],
            "js":["content-script.js"],
            "css":["content.css"]
            
            

        }
    ],
    "permissions": ["storage", "activeTab", "scripting","tabs"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/Asset/icon.png",
            "32": "/Asset/icon.png",
            "48": "/Asset/icon.png",
            "128": "/Asset/icon.png"
          }
    },
    "icons": {
        "16": "/Asset/icon.png",
        "32": "/Asset/icon.png",
        "48": "/Asset/icon.png",
        "128": "/Asset/icon.png"
    }

    

}